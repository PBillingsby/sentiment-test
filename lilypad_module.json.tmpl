{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 6000
    },
    "job": {
        "APIVersion": "V1beta1",
        "Metadata": {
            "CreatedAt": "0001-01-01T00:00:00Z",
            "Requester": {}
        },
        "Spec": {
            "Deal": { "Concurrency": 1 },
            "Docker": {
                "WorkingDirectory": "/workspace",
                "Entrypoint": ["python", "/workspace/src/run_inference.py"],
                "EnvironmentVariables": [
                    "INPUT={{ js .input }}"
                ],
                "Image": "pbillingsby/sentiment-test:latest"
            },
            "Engine": "Docker",
            "Network": { "Type": "None" },
            "Outputs": [
                { "Name": "outputs", "Path": "/outputs" }
            ],
            "Resources": { "GPU": "1" },
            "Timeout": 600,
            "Wasm": { "EntryModule": {} }
        }
    }
}
